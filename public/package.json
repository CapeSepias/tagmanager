{
  "name": "tagmanager",
  "version": "0.0.1",
  "description": "Tool for managing Guardian tags",
  "scripts": {
    "build-js": "webpack ./app.js ./app.compiled.js --config ./build_config/webpack.prod.conf.js",
    "build-js-dev": "webpack ./app.js ./app.compiled.js --config ./build_config/webpack.dev.conf.js --watch",
    "build-icons": "svg-sprite -w 16 -h 16 -v true --view-bust false --view-prefix .i-%s --view-dest . --vs ./images/icons.svg --vscss true --view-render-scss-template ./images/icons/scssTemplate.scss --view-render-scss-dest ./style/_icons.scss  ./images/icons/*.svg",
    "client-dev-js": "node ./devserver.js"
  },
  "dependencies": {
    "history": "^1.13.0",
    "node-sass": "^3.3.3",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "reqwest": "^2.0.5",
    "svg-sprite": "^1.2.12"
  },
  "devDependencies": {
    "babel-loader": "^5.3.3",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "expose-loader": "^0.7.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
